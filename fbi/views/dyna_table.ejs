<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">
<table class="table table-dark table-stripped">
    <thead>
        <tr>
            <th>
                Name
            </th>
            <th>
                Age
            </th>
            <th>
                Image
            </th>
        </tr>
    </thead>
    <tbody>
        <%for (i of data){%>
            <tr>
                <th>
                    <%=i.name%>
                </th>
                
                <th>
                    <%=i.description%>
                </th>
                <th>
                    <img width="150" height="100" src=<%=i.image%>>
                </th>
            </tr>
        <%}%>
    </tbody>
</table>